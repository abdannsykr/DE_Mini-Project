<?xml version="1.0" encoding="UTF-8"?>
<uml:UseCaseDiagram xmlns:uml="http://www.omg.org/spec/UML/20161101">
    <uml:UseCaseDiagram.name>Data Engineer Use Case</uml:UseCaseDiagram.name>
    
    <!-- Actors -->
    <uml:Actor id="actor1" name="Data Engineer">
        <uml:Actor.name>Data Engineer</uml:Actor.name>
    </uml:Actor>

    <!-- Use Cases -->
    <uml:UseCase id="useCase1">
        <uml:UseCase.name>Design High-Level Architecture</uml:UseCase.name>
    </uml:UseCase>
    
    <uml:UseCase id="useCase2">
        <uml:UseCase.name>Set Up Environment</uml:UseCase.name>
    </uml:UseCase>
    
    <uml:UseCase id="useCase3">
        <uml:UseCase.name>Extract Data</uml:UseCase.name>
    </uml:UseCase>
    
    <uml:UseCase id="useCase4">
        <uml:UseCase.name>Transform Data</uml:UseCase.name>
    </uml:UseCase>
    
    <uml:UseCase id="useCase5">
        <uml:UseCase.name>Load Data</uml:UseCase.name>
    </uml:UseCase>
    
    <uml:UseCase id="useCase6">
        <uml:UseCase.name>Analyze Data</uml:UseCase.name>
    </uml:UseCase>
    
    <uml:UseCase id="useCase7">
        <uml:UseCase.name>Visualize Data</uml:UseCase.name>
    </uml:UseCase>
    
    <!-- Associations -->
    <uml:Association id="association1" from="actor1" to="useCase1"/>
    <uml:Association id="association2" from="actor1" to="useCase2"/>
    <uml:Association id="association3" from="actor1" to="useCase3"/>
    <uml:Association id="association4" from="actor1" to="useCase4"/>
    <uml:Association id="association5" from="actor1" to="useCase5"/>
    <uml:Association id="association6" from="actor1" to="useCase6"/>
    <uml:Association id="association7" from="actor1" to="useCase7"/>
    
</uml:UseCaseDiagram>
